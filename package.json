{
  "name": "vitejs-plugins",
  "version": "0.1.1",
  "description": "easy to use vitejs plugins",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "release": "yarn unbuild && npx release-it"
  },
  "keywords": [
    "vitejs",
    "plugin",
    "vite",
    "plugins"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zuolung/vite-plugins"
  },
  "bugs": {
    "url": "https://github.com/zuolung/vite-plugins/issues"
  },
  "author": "zuolung",
  "license": "MIT",
  "dependencies": {
    "@babel/generator": "^7.20.7",
    "@babel/parser": "^7.20.7",
    "@babel/template": "^7.20.7",
    "@babel/traverse": "^7.20.12",
    "@babel/types": "^7.20.7",
    "chokidar": "^3.5.3",
    "glob": "^8.0.3",
    "picocolors": "^1.0.0",
    "picomatch": "^2.3.1",
    "prettier": "^2.8.1"
  },
  "devDependencies": {
    "release-it": "^15.6.0",
    "unbuild": "^1.0.2",
    "vite": "^4.0.4"
  },
  "peerDependencies": {
    "vite": ">=3.0.0"
  }
}
